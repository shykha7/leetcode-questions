/**
 * @param {number[]} nums
 * @return {number}
 */
var findMaxConsecutiveOnes = function(nums) {
    let ConsecutiveCount = 0;
    let maxCount = 0
    for(let i = 0;i<=nums.length;i++){
        if(nums[i]===1){
            ConsecutiveCount++;
             if(ConsecutiveCount > maxCount){
             maxCount= ConsecutiveCount
             }
        }else{
        ConsecutiveCount = 0 

        }
    }
        return maxCount;

};
