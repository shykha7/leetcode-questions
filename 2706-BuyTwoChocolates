/**
 * @param {number[]} prices
 * @param {number} money
 * @return {number}
 */
var buyChoco = function(prices, money) {
    let minSum = Infinity
    for(let i = 0; i<prices.length; i++){
        for(let j = i+1; j<prices.length; j++){
            let sum = prices[i]+prices[j]
            if(sum <= money){
                minSum = Math.min(minSum,sum)
            }
        }
    }

    if(minSum === Infinity){
        return money
    }

     return money-minSum
};
