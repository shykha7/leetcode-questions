/**
 * @param {string} s
 * @return {boolean}
 */
var areNumbersAscending = function(s) {
    let arr = []
    let num = ""
    
    for(let i =0;i<s.length;i++){
        if(!isNaN(s[i])&&s[i]!==" "){
            num+=s[i]
        }else if(num){
            arr.push(Number(num))
            num = ""
        }
    }
    if(num){ arr.push(Number(num))}

    let ascn = arr.every((val,i,arr)=> i ===0 || arr[i-1] < val)
    return ascn
};
