/**
 * @param {number[]} digits
 * @return {number[]}
 */
var plusOne = function(digits) {
   let lastNum = digits.length-1
   let newArr = []
   for(let i=lastNum;i>=0;i--){
   if (digits[i] < 9) {
            digits[i] += 1;
            return digits;
    }
    digits[i]=0
   }

   digits.unshift(1);
    return digits;

};
