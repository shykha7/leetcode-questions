/**
 * @param {number[]} arr
 * @return {number}
 */
var sumOddLengthSubarrays = function(arr) {
    let n = arr.length;
    let sum = 0;

    for(let len=1;len<=n;len+=2){
        for(let i=0; i+len <= n; i++)
           for(let j=i; j < i+len;j++){
            sum+=arr[j]
           }
    }
    return sum;
};
